 generator client {
  provider = "prisma-client-js"
}   
   
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Employee {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  position  String
  salary    Float
  createdAt DateTime @default(now())
}
